# This program demonstrates the ability to execute treat MysoreScript functions as Epilog rules.
@[Epilog] {
	iss(A, A).
	?- iss(a, N), writeln(N).
@}
@[MysoreScript] {
	func square(x) {
		var y = 0;
		var i = 0;
		while (i < x) {
			y = y + x;
			i = i + 1;
		}
		return y;
	};
@}
@[Epilog] {
	?- square(5, 25).
@}
